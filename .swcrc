{
    "$schema": "https://json.schemastore.org/swcrc",
    "sourceMaps": true,
    "jsc": {
        "target": "es2018",
        "parser": {
            "syntax": "typescript",
            "decorators": true,
            "dynamicImport": true
        },
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        },
        "baseUrl": "./",
        "paths": {
            "@/*": ["src/*"]
          }
    },
    "module": {
        "type": "commonjs"
    },
    "minify": false
}
